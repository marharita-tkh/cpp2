#include <string>
#include <iostream>
using namespace std;

int main()
{
    int dec = 0;
    string bi;
    int c = 1;
    cout << "CIN binary"<<endl;
    cin >> bi;
    int len = bi.length();
    for(int i = len; i>0; i--, c *= 2)
    {
        if(bi[i - 1] == '1') 
        {
	        dec += c;
        }
    }
    cout << dec << endl;
    cout<<endl;
    return 0;
}
